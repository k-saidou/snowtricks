{{ form_start(form) }} 
        <div>
        {{ form_row(form.nom, {
            'attr':{'class':'form-control'}
        }) }}
    </div>  
    <br>  
    <div>
        {{ form_row(form.prenom, {
            'attr':{'class':'form-control'}
        }) }}
    </div>    
    <br>
    <div>
        {{ form_row(form.email, {
            'attr':{'class':'form-control'}
        }) }}
    </div>  
    <br>  
    <div>
        {{ form_row(form.password, {
            'attr':{'class':'form-control'}
        }) }}
    </div>
    <br>
    <button class="btn btn-outline-success">{{ button_label|default('Enregistrer') }}</button>
    <br><br>
{{ form_end(form) }}
